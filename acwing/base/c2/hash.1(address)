//init
const int N=2e+3,null=0x3f3f3f3f;
int h[N];
void init (){
    memset(h,0x3f,sizeof h);
}

//find()
int find(int x){
    int k=(x%N+N)%N;
    while(h[k]!=null&&h[k]!=x){
        k++;
        if(k==N)k=0;
    }
    return k;
}

//insert(int x)
h[find(x)]=x;

//find(x)
if(h[find(x)==null)NO
else YES
